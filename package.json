{
  "name": "tutereview",
  "version": "0.0.0",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev:server": "cd server && npm run dev",
    "dev:client": "cd client && npm start",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "build:client": "cd client && npm run build",
    "build:server": "cd server && npm run build",
    "build": "npm run build:client && npm run build:server && rsync -av --delete client/build/ server/build/",
    "test-production": "npm run build && cd server && npm start",
    "deploy": "npm run build && gcloud app deploy server/app.yaml"
  },
  "author": "Rohan Selva-Radov",
  "license": "ISC",
  "description": "",
  "type": "module",
  "devDependencies": {
    "@tsconfig/node22": "^22.0.0",
    "concurrently": "^8.2.2"
  }
}